{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dicas",
      "destination": "/dicas.html"
    },
    {
      "source": "/noticias",
      "destination": "/noticias.html"
    },
    {
      "source": "/sobre",
      "destination": "/sobre.html"
    },
    {
      "source": "/contato",
      "destination": "/contato.html"
    },
    {
      "source": "/politicas",
      "destination": "/politicas.html"
    },
    {
      "source": "/termos",
      "destination": "/termos.html"
    },
    {
      "source": "/artigo-aberturas",
      "destination": "/artigo-aberturas.html"
    },
    {
      "source": "/artigo-taticas",
      "destination": "/artigo-taticas.html"
    },
    {
      "source": "/artigo-meiojogo",
      "destination": "/artigo-meiojogo.html"
    },
    {
      "source": "/guia-tabuleiro",
      "destination": "/guia-tabuleiro.html"
    },
    {
      "source": "/guia-pecas",
      "destination": "/guia-pecas.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

